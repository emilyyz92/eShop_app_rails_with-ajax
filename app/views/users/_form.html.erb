<%= form_for user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <%= f.label :phone_number %>
  <%= f.telephone_field :phone_number %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  This is an admin account <%= f.check_box :admin %><br>
  <%= f.submit %>
<% end %>
